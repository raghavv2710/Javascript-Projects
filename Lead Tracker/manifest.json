{
    "manifest_version": 3,
    "version": "1.0",
    "name": "Leads tracker",
    "action": {
        "default_popup": "index.html",
        "default_icon": "favicon-16x16.png"
    },
    "permissions": [
        "tabs"
    ],
    "content_security_policy": {
  "script-src": "'self' 'wasm-unsafe-eval' 'inline-speculation-rules' http://localhost:* http://127.0.0.1:* https://www.gstatic.com"
    }
}